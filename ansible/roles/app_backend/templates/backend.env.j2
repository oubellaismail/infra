# Backend Environment Configuration
SPRING_PROFILES_ACTIVE={{ env_name }}
SERVER_PORT={{ app.port }}

# Database Configuration
DATABASE_URL=postgresql://{{ database.host }}:{{ database.port }}/{{ database.name }}
DATABASE_USERNAME={{ vault_db_user }}
DATABASE_PASSWORD={{ vault_db_password }}
DATABASE_POOL_SIZE={{ database.pool_size | default(10) }}

# Security
JWT_SECRET={{ vault_jwt_secret }}
CORS_ORIGINS={{ app_config.cors_origins | join(',') }}